data Nati : Type where
  Zeroi
  Succi of (Nati)

show : Nati -> Char
show = \s . case s of
    Zeroi -> '0'
    Succi n -> '1'

pred : Nati -> Char
pred = \n . case n of
  Zeroi -> show Zeroi
  Succi nn -> show nn
