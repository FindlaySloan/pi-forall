module Translate where

data Maybe (A : Type) : Type where
   Nothing
   Just of (A)

g : Bool -> Bool -> Bool
g = \a b . case a of
    True -> case b of
                True -> True
                False -> False
    False -> False

f : Maybe (Maybe Char) -> Char
f = \b . case b of
           Nothing  -> 'a'
           Just n   -> case n of
                           Nothing -> 'a'
                           Just g -> g
{--
